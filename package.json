{
  "name": "paranoia-character-sheet",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "compile": "bun compile-sass && bun compile-script && bun run ./tool/compile-html.ts",
    "compile-script": "rm -rf ./build/js && tsc && rollup --config tool/rollup.config.js",
    "compile-sass": "sass --no-source-map --style=compressed src/scss:build/css",
    "deploy": "ts-node-esm tool/deploy-character-sheet-via-browser.ts"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^2.2.2",
    "axios": "^1.3.4",
    "bun-types": "^0.5.0",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.0.3",
    "puppeteer": "^19.8.3",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "sass": "^1.60.0",
    "slashes": "^3.0.12",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {}
}
